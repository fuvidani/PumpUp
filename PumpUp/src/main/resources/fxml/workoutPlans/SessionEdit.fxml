<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="mainBox" maxHeight="1.7976931348623157E308"
      maxWidth="1.7976931348623157E308" stylesheets="@../../css/application.css" xmlns="http://javafx.com/javafx/8"
      fx:controller="sepm.ss15.grp16.gui.controller.workoutPlans.old.SessionEditController">
    <children>
        <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
            <children>
                <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Sessionname">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="txtName" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                           promptText="Session 1">
                    <font>
                        <Font size="13.0"/>
                    </font>
                    <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </HBox.margin>
                </TextField>
            </children>
            <VBox.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </VBox.margin>
        </HBox>
        <Separator maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
            <VBox.margin>
                <Insets bottom="5.0" top="5.0"/>
            </VBox.margin>
        </Separator>
        <HBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
              VBox.vgrow="ALWAYS">
            <children>
                <TableView fx:id="tblvExerciseTable" editable="true" maxHeight="1.7976931348623157E308"
                           maxWidth="1.7976931348623157E308">
                    <columns>
                        <TableColumn fx:id="tblcOrder" maxWidth="1.7976931348623157E308" minWidth="-1.0"
                                     prefWidth="30.0" text="Nr."/>
                        <TableColumn fx:id="tblcExercise" maxWidth="1.7976931348623157E308" minWidth="-1.0"
                                     prefWidth="200.0" sortable="false" text="Ãœbung"/>
                        <TableColumn fx:id="tblcType" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="70.0"
                                     sortable="false"/>
                    </columns>
                    <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </HBox.margin>
                </TableView>
                <VBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                      prefWidth="40.0">
                    <children>
                        <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                              spacing="10.0" VBox.vgrow="ALWAYS">
                            <children>
                                <Button fx:id="btnUp" maxHeight="1.7976931348623157E308"
                                        maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onClickUp"
                                        prefHeight="50.0" styleClass="btnUp">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </Button>
                                <Button id="btnDown" fx:id="btnDown" maxHeight="1.7976931348623157E308"
                                        maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                                        onAction="#onClickDown" prefHeight="50.0" styleClass="btnDown">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                              spacing="10.0" VBox.vgrow="ALWAYS">
                            <children>
                                <Button fx:id="btnAdd" maxHeight="1.7976931348623157E308"
                                        maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onClickAdd"
                                        prefHeight="50.0" styleClass="btnAdd">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </Button>
                                <Button id="btnShow" fx:id="btnEdit" disable="true" maxHeight="1.7976931348623157E308"
                                        maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                                        onAction="#onClickEdit" prefHeight="50.0" styleClass="btnEdit">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </Button>
                                <Button id="btnShow" fx:id="btnShow" disable="true" maxHeight="1.7976931348623157E308"
                                        maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                                        onAction="#onClickShow" prefHeight="50.0" styleClass="btnShow">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </Button>
                                <Button id="btnNew" fx:id="btnDelete" disable="true" maxHeight="1.7976931348623157E308"
                                        maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                                        onAction="#onClickDelete" prefHeight="50.0" styleClass="btnDelete">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>
                    </children>
                    <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </HBox.margin>
                </VBox>
            </children>
            <VBox.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </VBox.margin>
        </HBox>
    </children>
</VBox>
