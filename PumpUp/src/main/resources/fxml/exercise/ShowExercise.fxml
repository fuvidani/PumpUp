<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.WebView?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="700.0"
            prefWidth="1300.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="sepm.ss15.grp16.gui.controller.exercises.ShowExerciseController">
    <bottom>
        <HBox alignment="CENTER_RIGHT" spacing="30.0" BorderPane.alignment="CENTER_RIGHT">
            <children>
                <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                        onAction="#finishedClicked" text="Fertig">
                    <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </HBox.margin>
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Button>
            </children>
            <BorderPane.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </BorderPane.margin>
        </HBox>
    </bottom>
    <center>
        <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
            <children>
                <VBox fillWidth="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Daten">
                            <font>
                                <Font size="24.0"/>
                            </font>
                            <VBox.margin>
                                <Insets bottom="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </Label>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Name:">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="exerciseNameField" editable="false" maxHeight="1.7976931348623157E308"
                                   maxWidth="1.7976931348623157E308" promptText="Name der Übung">
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </TextField>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                               text="Beschreibung:">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </Label>
                        <TextArea fx:id="descriptionArea" editable="false" maxHeight="1.7976931348623157E308"
                                  maxWidth="1.7976931348623157E308"
                                  promptText="Eine ausführliche Anleitung, wie die Übung auszuführen ist."
                                  wrapText="true">
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </TextArea>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Video Link:">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="videoLinkField" editable="false" maxHeight="1.7976931348623157E308"
                                   maxWidth="1.7976931348623157E308" onAction="#playVideo"
                                   promptText="https://www.youtube.com/watch?v=Eh00_rniF8E&amp;ab_channel=ScottMalin">
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </TextField>
                        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                mnemonicParsing="false" onAction="#playVideo" text="play video">
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </Button>
                        <WebView fx:id="webViewVideo" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0"
                                 prefWidth="-1.0">
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </WebView>
                    </children>
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </VBox>
                <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                               text="Kategorien">
                            <font>
                                <Font size="24.0"/>
                            </font>
                        </Label>
                        <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <children>
                                <VBox fx:id="vboxType" maxHeight="1.7976931348623157E308"
                                      maxWidth="1.7976931348623157E308" spacing="5.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label text="Art"/>
                                    </children>
                                </VBox>
                                <VBox fx:id="vboxEquipment" maxHeight="1.7976931348623157E308"
                                      maxWidth="1.7976931348623157E308" spacing="5.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label text="Geräte"/>
                                    </children>
                                </VBox>
                                <VBox fx:id="vboxMuscle" maxHeight="1.7976931348623157E308"
                                      maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                                    <children>
                                        <Label text="Muskelgruppe"/>
                                    </children>
                                </VBox>
                            </children>
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="20.0"/>
                            </VBox.margin>
                        </HBox>
                        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Infos">
                            <font>
                                <Font size="24.0"/>
                            </font>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </Label>
                        <VBox fillWidth="false">
                            <children>
                                <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                       text="Kalorien pro Einheit">
                                    <VBox.margin>
                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                                    </VBox.margin>
                                </Label>
                                <TextField fx:id="caloriesField" editable="false" maxHeight="1.7976931348623157E308"
                                           maxWidth="1.7976931348623157E308" promptText="kcal">
                                    <VBox.margin>
                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                                    </VBox.margin>
                                </TextField>
                            </children>
                        </VBox>
                    </children>
                    <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </HBox.margin>
                </VBox>
                <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                               text="Bild">
                            <font>
                                <Font size="24.0"/>
                            </font>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </Label>
                        <ImageView fx:id="imageView" fitHeight="200.0" fitWidth="300.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </ImageView>
                        <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <children>
                                <ListView fx:id="imagesListView" maxHeight="1.7976931348623157E308"
                                          maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0">
                                    <HBox.margin>
                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                                    </HBox.margin>
                                </ListView>
                                <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                                    <HBox.margin>
                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                                    </HBox.margin>
                                </VBox>
                            </children>
                            <VBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </VBox.margin>
                        </HBox>
                    </children>
                    <HBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </HBox.margin>
                </VBox>
            </children>
            <BorderPane.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </BorderPane.margin>
        </HBox>
    </center>
    <top>
        <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
            <children>
                <Label prefHeight="23.0" prefWidth="183.0" text="Übung">
                    <font>
                        <Font size="30.0"/>
                    </font>
                </Label>
            </children>
            <BorderPane.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </BorderPane.margin>
        </HBox>
    </top>
</BorderPane>
